#!/usr/bin/env bash
# bin/release <build-dir>

cat <<EOF
---
config_vars:
  PATH: /usr/local/bin:/usr/bin:/bin:/app/bin:/app/vendor/freetds-0.91/bin
  LD_LIBRARY_PATH: /app/vendor/freetds-0.91/lib
  LIBRARY_PATH: /app/vendor/freetds-0.91/lib
  CPATH: /app/vendor/freetds-0.91/include
  CPPATH: /app/vendor/freetds-0.91/include
EOF